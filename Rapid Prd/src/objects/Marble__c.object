      ,
        AND(
             OR(
               Industry__r.Name = &apos;Auto Sales - New&apos;
              ,Industry__r.Name = &apos;Auto Sales - Used&apos;
              ,Industry__r.Name = &apos;Trucking&apos;
             ) 
            , FICO_Score__c &lt; 620
        )
