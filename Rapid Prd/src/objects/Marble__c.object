      , Average_Balancce__c &lt; 1000
      , Current_Bank_Balance__c &lt; 0
        /* uncomment if necessary
      , ispickval(Entity_Type__c,&apos;Non-Profit&apos;)
        */
        /* uncomment if necessary
      , Soft_Pull_Only__c = TRUE
        */
      , No_Daily_Payment__c  = TRUE
      , Last_Funded_Date__c &gt; (Today() - (30*1))
      ,
       AND( 
            Bankruptcy__c = TRUE
           , OR( Bankruptcy_Discharge_Date__c &gt; (today()-(30 * 6))
                ,ISNULL(Bankruptcy_Discharge_Date__c)
               )
           )
      ,
        AND(
              Tax_Liens__c = FALSE
            , Tax_Lien_Balance__c &gt; 0
        ) 
      , 
        AND(
              Tax_Liens__c = TRUE
            , Tax_Lien_Balance__c &lt; 0
        )
      ,
        AND(
              Industry__r.Name = &apos;Trucking&apos;
            , OR( 
                   QC_Monthly_Sales__c &lt; 50000
                 , Business_Open_Date__c &gt; (Today() - (30*12))
