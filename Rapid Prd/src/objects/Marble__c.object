    <fields>
        <fullName>BFS__c</fullName>
        <externalId>false</externalId>
        <formula>IF( 
OR(

/* uncomment if necessary
State_of_Operation__c = &apos;&apos;
*/
 
FICO_Score__c &lt; 550

/* uncomment if necessary 
, Credit_Inquiries_Last_90_Days__c &gt; 15 
*/

/* uncomment if necessary
, QC_Monthly_Sales__c &lt; 12000
*/

/* uncomment if necessary 
, Ave_Number_of_Deposits_per_Month__c &lt; 3 
*/

, Average_Monthly_Negative_Balance__c &gt; 3 
, Business_Open_Date__c &gt; (Today() - (30*24))  
, Industry__r.BFS__c = TRUE 
, Average_Balancce__c &lt; 1500
, Current_Bank_Balance__c &lt; 0
, ispickval(Entity_Type__c,&apos;Non-Profit&apos;)
, RapidAdvance_Balance__c = TRUE
, Last_Funded_Date__c &gt; (Today() - (30*2)) 

/* uncomment if necessary 
, Soft_Pull_Only__c = TRUE 
*/ 

,
AND( 
Bankruptcy__c = TRUE
,OR( Bankruptcy_Discharge_Date__c &gt; (today()-(30*0))
,ISNULL(Bankruptcy_Discharge_Date__c)
)
)

, 
AND( 
Tax_Liens__c = FALSE 
, Tax_Lien_Balance__c &gt; 100000 
) 
, 
AND( 
Tax_Liens__c = TRUE 
, Tax_Lien_Balance__c &lt; 0 
) 
,
        AND(
              OR(
                  Industry__r.Name = &apos;Trucking&apos; 
                 ,Industry__r.Name = &apos;Limousine&apos;
                 ,Industry__r.Name = &apos;Car Service&apos;
                 )
             ,OR( 
                  Business_Open_Date__c &gt; (today() - (30*36))
                 ,FICO_Score__c &lt; 625
                 )
            )
      ,
        AND(
              Industry__r.Name = &apos;Construction&apos; 
             ,OR( 
                  Business_Open_Date__c &gt; (today() - (30*60))
                 ,FICO_Score__c &lt; 625
                 )
        )
      , 
        IF( 
            Position__c &gt; 1 /*Fixed - DON&apos;T CHANGE VALUE FOR ANY PARTNER*/
          ,  
            Number_of_Competitor_Balances__c &gt;= 1 /*Deepest Position*/
          , 
            OR(
                 Number_of_Competitor_Balances__c &gt; 2 /*Max # of Payoffs*/
                 /*uncomment if necessary
               , Sum_of_Competitor_Balances__c &gt; 75000 
                 */
               , if(Number_of_Competitor_Balances__c &gt; 1
               , 1 -(Sum_of_Competitor_Balances__c / QC_Monthly_Sales__c) &lt; 0.75 /*Balance Net % with &gt; 1 competitor balances*/
               , 1 -(Sum_of_Competitor_Balances__c / QC_Monthly_Sales__c) &lt; 0.5 /*Balance Net % with 1 competitor balance*/
              )
              )
          )
) 
, &quot;&quot; 
, &quot;BFS APPROVED&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>BFS</label>
        <required>false</required>
