    <fields>
        <fullName>Accountable__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
   OR(  
        State_of_Operation__c = &apos;CA&apos;
      , State_of_Operation__c = &apos;NY&apos;
      , State_of_Operation__c = &apos;HI&apos;
      , FICO_Score__c &lt; 540
      , Credit_Inquiries_Last_90_Days__c &gt; 8
      , QC_Monthly_Sales__c &lt; 5000
      , Ave_Number_of_Deposits_per_Month__c &lt; 4
      , Average_Monthly_Negative_Balance__c &gt; 3
      , Business_Open_Date__c &gt; (Today() - (30*8))
      , Industry__r.Accountable__c = TRUE
      , Average_Balancce__c &lt; 1000
      , Current_Bank_Balance__c &lt; 0
      , ispickval(Citizenship_Status__c,&apos;Visa&apos;)
      , ispickval(Citizenship_Status__c,&apos;Green Card&apos;)
      , ispickval(Entity_Type__c,&apos;Non-Profit&apos;)
      , Soft_Pull_Only__c = TRUE
      , Last_Funded_Date__c &gt; (Today() - (30*3))

      ,
       AND( 
            Bankruptcy__c = TRUE
           , OR( Bankruptcy_Discharge_Date__c &gt; (today()-(1))
                ,ISNULL(Bankruptcy_Discharge_Date__c)
               )
           )
      ,
        AND(
              Tax_Liens__c = FALSE
            , Tax_Lien_Balance__c &gt; 0
        ) 
      , 
        AND(
              Tax_Liens__c = TRUE
            , Tax_Lien_Balance__c &gt; 0
        )
      , AND( FICO_Score__c &lt; 540
               ,OR(QC_Monthly_Sales__c &lt; 10000
                 , Ave_Number_of_Deposits_per_Month__c &lt; 10
                 , Average_Monthly_Negative_Balance__c &gt; 2
                 , Business_Open_Date__c &gt; (Today() - (30*12))
                 , Average_Balancce__c &lt; 1500
                 )
              )
      ,
        IF( 
            Position__c &gt; 1 /*Fixed - DON&apos;T CHANGE VALUE FOR ANY PARTNER*/
          ,  
            Number_of_Competitor_Balances__c &gt;= 1 /*Deepest Position*/
          , 
            OR(
                 Number_of_Competitor_Balances__c &gt; 1 /*Max # of Payoffs*/
               , Sum_of_Competitor_Balances__c &gt; 15000 /*Max Payoff Amount*/
               , 1 -(Sum_of_Competitor_Balances__c / QC_Monthly_Sales__c) &lt; 0.4 /*Balance Net %*/
              )
          )
)
, &quot;&quot;
, IF(  ispickval(Decline_Reason__c,&quot;Qualified at Rapid&quot;) ,&quot;||OVERQUALIFIED||&quot;
, &quot;ACCOUNTABLE APPROVED&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Accountable</label>
