        IF( 
            Position__c &gt; 1 /*Fixed - DON&apos;T CHANGE VALUE FOR ANY PARTNER*/
          ,  
            Number_of_Competitor_Balances__c &gt;= 2 /*Deepest Position*/
          , 
            OR(
                 Number_of_Competitor_Balances__c &gt; 2 /*Max # of Payoffs*/
               , Sum_of_Competitor_Balances__c &gt; 250000 /*Max Payoff Amount*/
               , 1 -(Sum_of_Competitor_Balances__c / QC_Monthly_Sales__c) &lt; 0 /*Balance Net %*/
