            function addSection(id) 
            { 
            var elm = document.getElementById(id); 
            
            if (elm.getAttribute('nextId') == null) 
            { 
            elm.setAttribute('nextId', '2'); 
            } 
            if (elm.getAttribute('maxOccurs') == null) 
            { 
            elm.setAttribute('maxOccurs', ''); 
            } 
            var nextId = elm.getAttribute('nextId'); 
            addFromTemplate(elm.id); 
            } 
            
            function removeSection(id, removeId) 
            { 
            var elm = document.getElementById(id); 
            var btnRemove = document.getElementById(removeId); 
            removeTemplateObject(elm.id); 
            
            var count = 0; 
            var elms = document.getElementsByTagName(elm.tagName); 
            var prevSibling; 
            for (var i=0;i<elms.length;i++) 
            { 
            if(elms[i].id && elms[i].id.match(/^fldAddUser\d{3}FirstName$/i)) 
            { 
            prevSibling = elms[i]; 
            count++; 
            } 
            } 
            if (btnRemove.offsetHeight >= 1 && count < 2) 
            { 
            var clss = "" + btnRemove.parentNode.parentNode.parentNode.className; 
            btnRemove.parentNode.parentNode.parentNode.className = clss.replace(/screenOnly/g, ""); 
            btnRemove.parentNode.parentNode.parentNode.style.display = "none"; 
            } 
            } 
